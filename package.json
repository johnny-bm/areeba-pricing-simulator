        "react-hook-form": "^7.53.2",
        "react-phone-number-input": "^3.4.8",
        "react-resizable-panels": "^3.0.6",
        "react-router-dom": "^6.28.0",
        "recharts": "^2.15.2",
        "sonner": "^2.0.7",
        "tailwind-merge": "^2.5.4",
        "vaul": "^1.1.2",
        "zod": "^4.1.11"
