{
    "name": "areeba-pricing-simulator",
    "version": "2.2.0",
    "description": "Areeba Pricing Simulator - IT Services Quote and Proposal Generator",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:prod": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:watch": "vitest --watch",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "type-check": "tsc --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "analyze": "node scripts/analyze-bundle.js",
        "security:audit": "npm audit",
        "security:fix": "npm audit fix",
        "deploy:vercel": "vercel --prod",
        "deploy:netlify": "netlify deploy --prod",
        "deploy:check": "npm run lint && npm run type-check && npm run test && npm run build:prod",
        "precommit": "npm run lint:fix && npm run format && npm run type-check",
        "predeploy": "npm run deploy:check && npm run analyze"
    },
    "dependencies": {
        "@radix-ui/react-accordion": "^1.2.3",
