{
    "name": "areeba-pricing-simulator",
    "version": "2.2.0",
    "description": "areeba Pricing Simulator - IT Services Quote and Proposal Generator",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "node scripts/set-version.js && tsc -b && vite build",
        "build:prod": "node scripts/set-version.js && vite build --mode production",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:watch": "vitest --watch",
        "test:integration": "vitest --config config/vitest.config.ts run tests/integration",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "playwright test --debug",
        "verify-schema": "tsx scripts/verify-schema.ts",
        "lint": "eslint . --ext ts,tsx --fix",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "type-check": "tsc --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "analyze": "node scripts/analyze-bundle.js",
        "security:audit": "npm audit",
        "security:fix": "npm audit fix",
        "deploy:vercel": "vercel --prod",
        "deploy:vercel:script": "./scripts/deploy-vercel.sh",
        "deploy:quick": "./scripts/quick-deploy.sh",
        "deploy:netlify": "netlify deploy --prod",
        "deploy:check": "npm run lint && npm run type-check && npm run test && npm run build:prod",
        "precommit": "npm run lint:fix && npm run format && npm run type-check",
        "predeploy": "npm run deploy:check && npm run analyze",
        "vercel:build": "npm run build:prod",
        "db:types": "supabase gen types typescript --linked > src/types/database.ts",
        "db:types:check": "supabase gen types typescript --linked > src/types/database.check.ts && diff src/types/database.ts src/types/database.check.ts && rm src/types/database.check.ts",
        "db:pull": "supabase db pull"
    },
