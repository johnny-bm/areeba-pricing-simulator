# Environment Variables Template
# Copy this to .env.development and .env.production

# Feature Flags for New Architecture
VITE_USE_NEW_ARCHITECTURE=true
VITE_USE_NEW_PRICING=true
VITE_USE_NEW_AUTH=false
VITE_USE_NEW_ADMIN=false
VITE_USE_NEW_DOCUMENTS=false

# Development flags
VITE_ENABLE_DEBUG_LOGGING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here